{
  "MoesifOptions": {
    "ApplicationId": "<Your-Moesif-Application-Id>",
    "LocalDebug": false,
    "LogBody": true,
    "RequestMaxBodySize": 100,
    "ResponseMaxBodySize": 100,
    "LogBodyOutgoing": true,
    "ApiVersion": "3.1.0",
    "EnableBatching": true,
    "BatchSize": 1,
    "IsLambda": true
  },
  
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "AllowedHosts": "*"
  }
  