{
  "MoesifOptions": {
    "ApplicationId": "<APP_ID>",
    "LocalDebug": false,
    "LogBody": true,
    "RequestMaxBodySize": 1000,
    "ResponseMaxBodySize": 1000,
    "LogBodyOutgoing": true,
    "ApiVersion": "3.0.10",
    "EnableBatching": false,
    "BatchSize": 1,
    "IsLambda": true
  },
  
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "AllowedHosts": "*"
  }
  